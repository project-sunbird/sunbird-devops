{% if enable_proxyvalidator == "yes" %}
apiVersion: v1
kind: Service
metadata:
  labels:
    app: apimanager
  name: kong
  namespace: {{namespace}}
spec:
  ports:
  - name: http-proxyvalidator
    port: 8000
    protocol: TCP
    targetPort: 9090
  selector:
    app: apimanager
  type: ClusterIP
{% endif %}
