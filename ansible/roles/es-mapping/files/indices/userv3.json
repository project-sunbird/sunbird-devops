{
  "settings": {
    "index": {
      "number_of_shards": "5",
      "number_of_replicas": "1",
      "analysis": {
        "filter": {
          "mynGram": {
            "token_chars": [
              "letter",
              "digit",
              "whitespace",
              "punctuation",
              "symbol"
            ],
            "min_gram": "3",
            "type": "ngram",
            "max_gram": "10"
          }
        },
        "analyzer": {
          "cs_index_analyzer": {
            "filter": [
              "lowercase",
              "mynGram"
            ],
            "type": "custom",
            "tokenizer": "standard"
          },
          "keylower": {
            "filter": "lowercase",
            "type": "custom",
            "tokenizer": "keyword"
          },
          "cs_search_analyzer": {
            "filter": [
              "lowercase",
              "standard"
            ],
            "type": "custom",
            "tokenizer": "standard"
          }
        }
      }
    }
  },
  "aliases" : {
    "user_alias" : {}
  }
}