sunbird:TeacherShape {
    a [sunbird:Teacher] ;
    sunbird:teacherId @sunbird:isAText *;
    sunbird:firstName @sunbird:isAText *;
    sunbird:lastName @sunbird:isAText *;
    sunbird:fullName @sunbird:isAText *;
    sunbird:gender [sunbird:Male sunbird:Female sunbird:Transgender] *;
    schema:birthDate @sunbird:isADate *;
    sunbird:email @sunbird:isAnEmail *;
    sunbird:telephone @sunbird:isATelephone *;
    sunbird:externalIds @sunbird:ExternalIDShape *;
    sunbird:employmentState @sunbird:IndianStateEnum *;
    sunbird:teacherType [sunbird:HeadTeacher sunbird:ActingHeadTeacher sunbird:RegularTeacher
        sunbird:RTEPositionedTeacher sunbird:Principal sunbird:VicePrincipal sunbird:Lecturer sunbird:OtherTeacher] *;
    sunbird:appointmentType [sunbird:Regular sunbird:Contract sunbird:PartTime] *;
    sunbird:serviceJoiningDate @sunbird:isADate *;
    sunbird:disabilityType [sunbird:LocoMotorDisability sunbird:VisualDisability sunbird:OtherDisability sunbird:NotApplicable] *;
    sunbird:teacherSchoolBoardAffiliation [sunbird:StateBoard sunbird:CBSE sunbird:ICSE sunbird:IB sunbird:Cambridge sunbird:OtherBoard] *;
    sunbird:schoolCode @sunbird:isAText *;
    sunbird:highestAcademicQualification  @sunbird:AcadQualificationEnum *;
    sunbird:highestTeacherQualification [sunbird:TeacherNoProfQual sunbird:TeacherRelevantPursue sunbird:TeacherDiplomaSpEd
        sunbird:TeacherMEd sunbird:TeacherBEd sunbird:TeacherBElEd sunbird:TeacherDiploma] *;
    sunbird:highestMathQualification @sunbird:AcadQualificationEnum *;
    sunbird:highestScienceQualification @sunbird:AcadQualificationEnum *;
    sunbird:highestEnglishQualification @sunbird:AcadQualificationEnum *;
    sunbird:highestSSTQualification @sunbird:AcadQualificationEnum *;
    sunbird:highestVernacularLanguageQualification @sunbird:AcadQualificationEnum *;
    sunbird:vernacularLanguageStudied @sunbird:IndianVernacularLangEnum *;
    sunbird:isEnrolledInDELEDProgramme @sunbird:isABoolean *;
    sunbird:teachingExperience @sunbird:SchoolTeachingExperienceShape *;
    sunbird:schoolJoiningDate @sunbird:isADate *;
    sunbird:isMasterTrainer @sunbird:isABoolean *;
    sunbird:masterTrainerSubjects @sunbird:TrainingSubjectEnum *;
    sunbird:trainingsCompleted @sunbird:TrainingSubjectEnum *;
    sunbird:isTeacherInBRC @sunbird:isABoolean *;
    sunbird:isTeacherInCRC @sunbird:isABoolean *;
    sunbird:teacherStatus @sunbird:TeacherStatusEnum *;
    sunbird:authenticationStatus @sunbird:AuthenticationStatusEnum *;
    sunbird:activeStatus @sunbird:ActiveStatusEnum *;
    sunbird:lastUpdatedOn @sunbird:isADate *;
}

sunbird:ExternalIDShape {
    a [sunbird:ExternalID] ;
    sunbird:eid @sunbird:isAText *;
    sunbird:idType @sunbird:isAText *;
    sunbird:provider @sunbird:isAText *;
}

sunbird:SchoolTeachingExperienceShape {
    a [sunbird:TeachingExperience];
    sunbird:classesTaught [sunbird:Class1 sunbird:Class2 sunbird:Class3 sunbird:Class4 sunbird:Class5
        sunbird:Class6 sunbird:Class7 sunbird:Class8 sunbird:Class9 sunbird:Class10 sunbird:Class11 sunbird:Class12] *;
    sunbird:subjectsTaught @sunbird:IndianSubjectEnum *;
}