[Unit]
Description=Faust Survey Evidence Service
[Service]
Environment="_JAVA_OPTIONS='-Dlog4j2.formatMsgNoLookups=true'"
User=data-pipeline
Group=data-pipeline
Restart=on-failure
Type=simple
ExecStart=/opt/sparkjobs/faust_as_service/faust.sh  survey/py_survey_evidence_streaming survey/
[Install]
WantedBy=multi-user.target
