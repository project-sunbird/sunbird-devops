background_actor_provider=off
api_actor_provider=local
ekstep_api_base_url={{sunbird_ekstep_api_base_url}}
ekstep_authorization={{sunbird_ekstep_api_key}}
sunbird_telemetry_dispatchers=kafka
sunbird_telemetry_kafka_servers_config={{ groups['kafka'] | map('extract', hostvars, ['ansible_ssh_host']) | join(':9092,') }}
sunbird_telemetry_kafka_topic={{sunbird_telemetry_kafka_topic}}
