campaign.url=http://uci-service:9999

#Kafka Configuration
spring.kafka.bootstrap-servers={{sunbird_processing_kafka_host}}
spring.kafka.consumer.auto-offset-reset=earliest
spring.kafka.consumer.value-deserializer=org.springframework.kafka.support.serializer.JsonDeserializer
spring.kafka.producer.value-serializer=org.springframework.kafka.support.serializer.JsonSerializer
schema.registry.url={{zookeepers}}
outbound-error={{env_name}}.outbound-error
outboundProcessed={{env_name}}.outbound-processed
outboundUnprocessed={{env_name}}.outbound-unprocessed
campaign={{env_name}}.campaign

# Cassandra Configuration
spring.data.cassandra.contactpoints={{sunbird_cassandra_host}}
spring.data.cassandra.port=9042
spring.data.cassandra.keyspace-name={{env}}_uci_store
spring.data.cassandra.pool.heartbeat-interval=30s
spring.data.cassandra.pool.max-queue-size=2000
spring.data.cassandra.pool.idle-timeout=10s
spring.data.cassandra.pool.pool-timeout=1s

# Service Configuration
server.port=9090
logging.level.root=INFO